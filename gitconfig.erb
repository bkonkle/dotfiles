[alias]
  st = status -sb
  co = checkout
  lg = log --oneline --decorate
[apply]
	whitespace = nowarn
[color]
	ui = auto
[core]
	excludesfile = <%= ENV['HOME'] %>/.gitignore
	editor = mate -w
[github]
	user = bkonkle
	token = <%= file = File.open("gitapikey", "r"); file.gets %>
[push]
  default = tracking
[user]
	name = bkonkle
	email = brandon.konkle@gmail.com
