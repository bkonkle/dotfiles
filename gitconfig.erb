[alias]
  st = status -sb
  co = checkout
  lg = log --oneline --decorate
  local-branches = !git branch -vv | cut -c 3- | awk '$3 !~/\\[/ { print $1 }'
[apply]
  whitespace = nowarn
[color]
  ui = auto
[core]
  excludesfile = <%= ENV['HOME'] %>/.gitignore
  editor = vim
[github]
  user = bkonkle
  token = <%= @github %>
[push]
  default = tracking
[user]
  name = Brandon Konkle
  email = brandon@konkle.us
[diff]
	tool = Kaleidoscope