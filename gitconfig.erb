[alias]
  st = status -sb
  co = checkout
  lg = log --oneline --decorate
[apply]
	whitespace = nowarn
[color]
	ui = auto
[core]
	excludesfile = <%= ENV['HOME'] %>/.gitignore
	editor = mate -w
[github]
	user = bkonkle
	token = <%= print("Github API key: "); STDOUT.flush; STDIN.gets.chomp %>
[push]
  default = tracking
[user]
	name = bkonkle
	email = brandon@brandonkonkle.com
